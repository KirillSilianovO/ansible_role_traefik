providers:
  docker:
    exposedByDefault: false
api:
  dashboard: true
  insecure: true
  debug: true

entryPoints:
  web:
    address: ":80"
  websecure:
    address: ":443"
certificatesResolvers:
  letsEncryptResolver:
    acme:
      email: "{{ traefik_app_acme_email }}"
      storage: /etc/traefik/acme.json
      httpChallenge:
        entryPoint: web
log:
  level: DEBUG
